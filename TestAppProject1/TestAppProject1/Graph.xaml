<UserControl x:Class="TestAppProject1.Graph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestAppProject1"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Background="Crimson" Orientation="Horizontal" VerticalAlignment="Center">
        <TextBox x:Name="Advarsel" Height="400" Width="181" Background="LightGray" Margin="0,0,30,0" />
        <Canvas x:Name="canGraph" Background="White"
      Width="300" Height="200"
      VerticalAlignment="Center" HorizontalAlignment="Right"/>
        <Button Content="Opdater Graf" HorizontalAlignment="Left" Margin="-300,0,0,60" VerticalAlignment="Bottom" Width="89" Height="21" Click="Window_Loaded" RenderTransformOrigin="-3.708,4.559" Background="HotPink"/>
    </StackPanel>
</UserControl>
